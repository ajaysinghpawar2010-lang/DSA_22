#include <iostream>
#include <bits/stdc++.h>
using namespace std;

std::vector<int> v= {2,5,7,8,10,20,40,60} ;

std::vector <int> f(std::vector <int> &v, int target){
    int l=0;
    int r=v.size()-1;
    
    while (l<r){
        int sum = v[l] + v[r];
        if (sum == target){
            
            return {l+1,r+1};
        }
        else if (sum >target){
            r--;
        }
        else
            l++;
        
    }
    return {};
}

int main()
{

  std::vector <int>s = f(v,18);
  for(int i:s)
    std::cout << i<< std::endl;
  return 0;

}
