#include <iostream>
#include <bits/stdc++.h>
using namespace std;

std::vector<int> v= {7,1,5,6,3,2} ;

int BestTimeToBuySellMaxProfit (std::vector <int> &v){
    int min =v[0];
    int profit = 0;
    for(int i=1;i<v.size();i++){
        int curr_proft = v[i] - min;
        if(curr_proft > profit) profit = curr_proft;
        min = std::min(min,v[i]);
    }
    return profit;
}




int main()
{

  int s= BestTimeToBuySellMaxProfit(v);
  std::cout << s << std::endl;
  return 0;

}
