#include <iostream>
#include <bits/stdc++.h>
using namespace std;

std::vector<int> v= {3,0,5,0,0,6,1,0} ;

void Movezeros (std::vector <int> &v, int target){
    int index = 0;
    for(int i=0;i<v.size();i++){
        if(v[i] != 0){
          std::swap(v[index],v[i]);    
          index++;
        }
    }
}




int main()
{

  Movezeros(v,18);
  for(int i:v)
    std::cout << i<< std::endl;
  return 0;

}
